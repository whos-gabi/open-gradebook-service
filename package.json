{
  "name": "open-gradebook-service",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "node tests/helpers/scripts/test-init.js && cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "@apollo/server": "4.11.0",
    "@prisma/client": "5",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "graphql": "^16.12.0",
    "jsonwebtoken": "^9.0.3",
    "nodemon": "^3.1.11"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prisma": "5",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/helpers/jest.setup.js"
    ]
  }
}
